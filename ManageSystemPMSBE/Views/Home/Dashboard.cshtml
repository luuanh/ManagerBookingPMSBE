                        </select>
                    </div>
                </div>
                <div class="col-lg-4 col-md-3">
                    <div class="form-group has-feedback">
                        <label>Từ khóa</label>
                        <div class="input-group">
                            <input type="text" class="form-control" placeholder="Tên/ Mã khách sạn">
                            <span class="input-group-btn">
                                <button id="Search" ng-click="GetAllHotelUseBE()" class="btn btn-info btn-flat" type="button">
                                    <span class="fa fa-search"></span>
                                </button>
                            </span>
                        </div>
                    </div>
                </div>
            </div>
            <div>
                <h3>Danh sách khách sạn sử dụng BE</h3>
                <div ui-grid="gridOptionsBE" set-grid-height ui-grid-selection ui-grid-resize-columns ui-grid-move-columns
                     ui-grid-pinning ui-grid-pagination ui-grid-exporter ui-grid-auto-resize class="grid"></div>
            </div>
            HotelUseBE
        </div>
        <div id="HotelUsePMSBE" class="tab-pane fade  ">
            <div class="row">
                <div class="col-lg-2 col-md-3">
                    <div class="form-group has-feedback">
