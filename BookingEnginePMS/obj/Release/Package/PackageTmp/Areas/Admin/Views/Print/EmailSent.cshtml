@using BookingEnginePMS.Models;
@{
    Layout = "~/Areas/Admin/Views/Shared/_LayoutPrint.cshtml";
    ReservationEmailSent reservationEmail = (ReservationEmailSent) ViewData["reservationEmail"];
    if(reservationEmail is null)
    {
        reservationEmail = new ReservationEmailSent();
    }
}
<div class="uk-grid" id="ConfirmRoom">
    <div class="uk-width-1">
        <div>
            @Html.Raw(reservationEmail.Content)
        </div>
    </div>
</div>