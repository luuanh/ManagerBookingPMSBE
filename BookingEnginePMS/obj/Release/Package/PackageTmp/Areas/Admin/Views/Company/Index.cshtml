@using BookingEnginePMS.Models;
@{
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
    List<TransitionLanguage> transitions = (List<TransitionLanguage>)Session["transitions"];
    Transition transition = new Transition(transitions);
}
<script>
    $("#menu_5").addClass('click-menu-top')
</script>
<script>
    $(function () {
        var e = $("#kUI_window")
            , t = $("#kUI_undo");
        if (e.length) {
            var a = e
                , n = t.bind("click", function () {
                    a.data("kendoWindow").open(),
                        n.hide()
                });
            a.data("kendoWindow") || a.kendoWindow({
                width: "600px",
                title: "Information",
                actions: ["Pin", "Minimize", "Maximize", "Close"],
                close: function () {
                    n.show()
                }
            })
        }
    })
</script>
<style>
    input[type=checkbox] {
        position: absolute;
    }
</style>
<script src="~/Areas/Admin/Content/js/Company/company.js"></script>
<div id="kUI_window" style="display:none">
    <div>
        <div class="uk-grid uk-margin-bottom">
            <div class="uk-width-1 uk-margin-medium-bottom">
                <h4><b>@(transition.Translate(611, "Agency Information"))</b></h4>
                <hr />
            </div>
            <div class="uk-width-1-2 uk-margin-bottom">
                <div class="uk-grid uk-margin-bottom">
                    <div class="uk-width-1 uk-margin-medium-bottom">
                        <label><b>@(transition.Translate(612, "Group Guest"))</b></label>
                        <select class="md-input" ng-model="company.GroupGuestId" convert-to-number style="width:100%">
                            <option value="-1">N/A</option>
                            <option value="1">@(transition.Translate(613, "Guest restaurant"))</option>
                            <option value="2">@(transition.Translate(614, "Corporate"))</option>
                            <option value="3">@(transition.Translate(615, "Supplier"))</option>
                            <option value="4">@(transition.Translate(616, "FIt"))</option>
                            <option value="5">@(transition.Translate(617, "Agent/Company"))</option>
                            <option value="6">@(transition.Translate(618, "Online"))</option>
                            <option value="7">@(transition.Translate(619, "Guest workshop"))</option>
                            <option value="8">@(transition.Translate(620, "ORDER SALE"))</option>
                            <option value="9">@(transition.Translate(621, "Tour"))</option>
                            <option value="10">@(transition.Translate(622, ""))</option>
                        </select>
                        <span class="md-input-bar "></span>
                    </div>
                    <div class="uk-width-1 uk-margin-medium-bottom">
                        <label><b>@(transition.Translate(623, "Source"))</b></label>
                        <select class="md-input" ng-model="company.SourceId" convert-to-number style="width:100%;">
                            <option value="-1">N/A</option>
                            <option value="1">OTA</option>
                            <option value="2">TA</option>
                            <option value="3">GIT</option>
                            <option value="4">FIT</option>
                            <option value="5">WALK-IN</option>
                            <option value="6">COMP</option>
                            <option value="7">VOUCHER</option>
                            <option value="8">HU</option>
                            <option value="9">CORP</option>
                        </select>
                        <span class="md-input-bar "></span>
                    </div>
                    <div class="uk-width-1 uk-margin-medium-bottom">
                        <label><b>@(transition.Translate(624, "Name")) <span class="star-red">*</span></b></label>
                        <input ng-click="RefreshValidate()" ng-model="company.CompanyName" type="text" class="md-input">
                        <span class="md-input-bar "></span>
                        <span id="err_CompanyName" class="error">@(transition.Translate(625, "Value is invalid"))!</span>
                    </div>
                    <div class="uk-width-1 uk-margin-medium-bottom">
                        <label><b>@(transition.Translate(626, "Tax Code"))</b></label>
                        <input ng-click="RefreshValidate()" ng-model="company.TaxCode" type="text" class="md-input">
                        <span class="md-input-bar "></span>
                    </div>
                </div>
            </div>
            <div class="uk-width-1-2 uk-margin-bottom">
                <div class="uk-grid uk-margin-bottom">
                    <div class="uk-width-1 uk-margin-medium-bottom">
                        <label><b>@(transition.Translate(627, "Phone"))</b></label>
                        <input ng-click="RefreshValidate()" ng-model="company.Phone" type="text" class="md-input">
                        <span class="md-input-bar "></span>
                    </div>
                    <div class="uk-width-1 uk-margin-medium-bottom">
                        <label><b>@(transition.Translate(628, "Fax"))</b></label>
                        <input ng-click="RefreshValidate()" ng-model="company.Fax" type="text" class="md-input">
                        <span class="md-input-bar "></span>
                    </div>
                    <div class="uk-width-1 uk-margin-medium-bottom">
                        <label><b>@(transition.Translate(629, "Email"))</b></label>
                        <input ng-click="RefreshValidate()" ng-model="company.Email" type="text" class="md-input">
                        <span class="md-input-bar "></span>
                    </div>
                    <div class="uk-width-1 uk-margin-medium-bottom">
                        <label><b>@(transition.Translate(630, "Address"))</b></label>
                        <textarea ng-click="RefreshValidate()" ng-model="company.Address" rows="3" class="md-input"></textarea>
                        <span class="md-input-bar "></span>
                    </div>
                </div>
            </div>
            <div class="uk-width-1 uk-margin-bottom">
                <h5><b>***@(transition.Translate(631, "Contact User Information"))</b></h5>
            </div>
            <div class="uk-width-1 uk-margin-bottom">
                <div class="uk-grid uk-margin-bottom">
                    <div class="uk-width-1 uk-margin-medium-bottom">
                        <label><b>@(transition.Translate(632, "Contact Us Name"))</b></label>
                        <input ng-click="RefreshValidate()" ng-model="company.ContactUsName" type="text" class="md-input">
                        <span class="md-input-bar "></span>
                    </div>
                    <div class="uk-width-1 uk-margin-medium-bottom">
                        <label><b>@(transition.Translate(627, "Phone"))</b></label>
                        <input ng-click="RefreshValidate()" ng-model="company.ContactPhone" type="text" class="md-input">
                        <span class="md-input-bar "></span>
                    </div>
                    <div class="uk-width-1 uk-margin-medium-bottom">
                        <label><b>@(transition.Translate(629, "Email"))</b></label>
                        <input ng-click="RefreshValidate()" ng-model="company.ContactEmail" type="text" class="md-input">
                        <span class="md-input-bar "></span>
                    </div>
                </div>
            </div>
        </div>
        <div class="uk-grid">
            <div class="uk-width-1 uk-text-right">
                <button class="md-btn md-btn-new" ng-click="Post()" ng-if="isAdd">@(transition.Translate(633, "Add"))</button>
                <button class="md-btn md-btn-new" ng-click="Put()" ng-if="!isAdd">@(transition.Translate(634, "Update"))</button>
                <button class="md-btn md-btn-danger" ng-click="CloseWindow()">@(transition.Translate(635, "Cancel"))</button>
            </div>
        </div>
    </div>
</div>
<div class="uk-grid">
    <div class="uk-width-1-1 uk-margin-medium-bottom">
        <h3>@(transition.Translate(636, "Agency"))</h3>
        <hr />
    </div>
    <div class="uk-width-1-1 uk-margin-small-bottom">
        <div class="uk-grid">
            <div class="uk-width-1-3">
                <div class="uk-input-group">
                    <div class="md-input-wrapper">
                        <label><b>@(transition.Translate(637, "Key Search"))</b></label>
                        <input ng-model="filters.keySearch" type="text" class="md-input" placeholder="Name">
                        <span class="md-input-bar "></span>
                    </div>
                    <span class="uk-input-group-addon" ng-click="GetData()">
                        <a class="md-btn md-btn-new md-btn-wave-light">@(transition.Translate(638, "Search"))</a>
                    </span>
                </div>
            </div>
            <div class="uk-width-2-3 uk-text-right">
                <button class="md-btn md-btn-new md-btn-wave-light" ng-click="ShowAdd()">@(transition.Translate(639, "Add New"))</button>
            </div>
        </div>
    </div>
    <div class="uk-width-1-1">
        <div ui-grid="gridOptions" ui-grid-selection ui-grid-resize-columns ui-grid-move-columns
             ui-grid-pinning ui-grid-pagination ui-grid-exporter class="grid box-grid"></div>
    </div>
</div>