@using BookingEnginePMS.Models;
@{ 
    Hotel hotel = new Hotel();
    if (Session["HotelForBooking"] != null)
    {
        hotel = (Hotel)Session["HotelForBooking"];
    }
}
@model Transition
<h1 class="hotelname">@hotel.Name</h1>
<span class="hotel-address">@Model.Translate(2289, "Phone"): <a href="tel:@hotel.Phone">@hotel.Phone</a> | Email: <a href="@hotel.Email">@hotel.Email</a></span>
<br />
<span class="hotel-address">@hotel.Address</span>
